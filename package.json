{
  "name": "zboxapp",
  "version": "1.0.0",
  "description": "Aplicación para Zbox Postulación Trabajo 09/2015",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-linux": "electron-packager . gp-zboxapp --platform=linux --arch=x64 --version=0.31.1 --out=dist --ignore=\"node_modules/(electron-packager|electron-prebuilt|asar)\" --asar",
    "build-darwin": "electron-packager . gp-zboxapp --platform=darwin --arch=x64 --version=0.31.1 --out=dist --ignore=\"node_modules/(electron-packager|electron-prebuilt|asar)\" --asar",
    "build-win32": "electron-packager . gp-zboxapp --platform=win32 --arch=x64 --version=0.31.1 --out=dist --ignore=\"node_modules/(electron-packager|electron-prebuilt|asar)\" --asar"
  },
  "author": "Gonzalo Pincheira A.",
  "license": "ISC",
  "dependencies": {
    "electron-prebuilt": "^0.31.2",
    "twitter": "^1.2.5"
  },
  "devDependencies": {
    "asar": "^0.8.0",
    "electron-packager": "^5.0.2",
    "gulp": "^3.8.11",
    "gulp-inject": "^1.2.0",
    "gulp-jshint": "^1.9.4",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^1.0.0",
    "gulp-protractor": "^1.0.0",
    "gulp-uglify": "^1.1.0",
    "main-bower-files": "^2.6.2",
    "protractor": "^2.2.0",
    "protractor-html-screenshot-reporter": "0.0.21"
  }
}
